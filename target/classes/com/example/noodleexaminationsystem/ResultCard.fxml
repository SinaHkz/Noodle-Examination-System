<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<!--pane act like a button and open course plan-->

<Pane fx:id="card" prefHeight="178.0" prefWidth="301.0" stylesheets="@../../../style-sheets/card.css" visible="false" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.noodleexaminationsystem.CardController">
   <children>
      <Button layoutX="1.0" layoutY="-1.0" mnemonicParsing="false" prefHeight="178.0" prefWidth="301.0" styleClass="transparent" />
      <Button fx:id="courseName" layoutX="-1.0" layoutY="72.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="299.0" styleClass="white-background" text="CoursePlanName">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
   <styleClass>
      <String fx:value="white-background" />
      <String fx:value="border-radius" />
   </styleClass>
</Pane>
